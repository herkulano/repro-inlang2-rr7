{
	"name": "@acme/i18n",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"exports": {
		"./*": {
			"types": "./dist/*.d.ts",
			"default": ["./src/*.ts"]
		},
		"./runtime": {
			"types": "./dist/paraglide/runtime.d.ts",
			"default": ["./src/paraglide/runtime.js"]
		},
		"./messages": {
			"types": "./dist/paraglide/messages.d.ts",
			"default": ["./src/paraglide/messages.js"]
		}
	},
	"scripts": {
		"tsc": "tsc",
		"postinstall": "pnpm paraglide",
		"paraglide": "node --experimental-strip-types ./scripts/compile.ts",
		"typecheck": "tsc --noEmit --emitDeclarationOnly false"
	},
	"dependencies": {
		"@formatjs/intl-localematcher": "0.5.10",
		"@mjackson/headers": "0.9.0"
	},
	"devDependencies": {
		"@inlang/paraglide-js": "2.0.0-beta.8"
	}
}
